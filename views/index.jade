extends layout
//-https://github.com/vlavoie/inf4375/blob/master/views/index.jade
block content
    h1= title
    div#modif(style='width:20%;float:right;')
    div(style='width:70%;')
        table
            tr
                td
                    p Entrez l'arrondissement SVP.
                    table
                        thead
                            tr
                                th
                                    p Arrondissement
                                th
                        tbody
                            tr
                                td
                                    input#arrond(type="text", placeholder="arrondissement")
                                td
                                    button#btnSubmit(type="submit", onclick="sendParam()") submit
                                    
                    table
                        tbody
                            td
                                form(name="formulaire")
                                label(for='listder') nom d'installation (choisissez 1)
                                select#listder(onchange="sendInstal()")
                                   
                                    each c in dd
                                        option(value="#{c}") #{c.nom}
        div#result
    